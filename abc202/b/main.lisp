(defun convert (x)
  (ecase x
    (#\0 #\0)
    (#\1 #\1)
    (#\6 #\9)
    (#\8 #\8)
    (#\9 #\6)))

(declaim (inline println))
(defun println (obj &optional (stream *standard-output*))
  (let ((*read-default-float-format* 'double-float))
    (prog1 obj
      (princ obj stream)
      (terpri stream))))

(defun main ()
  (let ((s (read-line)))
    (println (map 'string  #'convert (nreverse s)))))

#-swank (main)
